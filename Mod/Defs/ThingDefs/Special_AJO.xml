<?xml version="1.0" encoding="utf-8" ?>
<ThingDefinitions>

	<ThingDef Name="UnfinishedBase" Abstract="True">
		<thingClass>UnfinishedThing</thingClass>
		<category>Item</category>
		<label>Unfinished thing</label>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<useHitPoints>true</useHitPoints>
		<isUnfinishedThing>true</isUnfinishedThing>
		<selectable>true</selectable>
		<tradeability>Never</tradeability>
		<drawerType>RealtimeOnly</drawerType>
		<statBases>
			<MaxHitPoints>10</MaxHitPoints>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
		<thingCategories>
			<li>Unfinished</li>
		</thingCategories>
	</ThingDef>

	<ThingDef ParentName="UnfinishedBase">
		<defName>AJO_UnfinishedPainting</defName>
		<label>Unfinished painting</label>
		<graphicData>
			<texPath>Cupro/Item/Unfinished/UnfinishedPainting</texPath>
		</graphicData>
		<stuffCategories>
			<li>Fabric</li>
			<li>AJO_Paint</li>
		</stuffCategories>
	</ThingDef>

	<ThingDef ParentName="UnfinishedBase">
		<defName>AJO_UnfinishedBook</defName>
		<label>Unfinished book</label>
		<graphicData>
			<texPath>Cupro/Item/Unfinished/UnfinishedBook</texPath>
		</graphicData>
		<stuffCategories>
			<li>AJO_BlankBook</li>
		</stuffCategories>
	</ThingDef>
	
	<ThingDef ParentName="BuildingBase">
		<defName>AJO_MovingWater</defName>
		<label>moving water</label>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<texPath>Cupro/Null</texPath>
			<drawSize>(0,0)</drawSize>
		</graphicData>
		<size>(1,1)</size>
		<useHitPoints>false</useHitPoints>
		<statBases>
			<Beauty>35</Beauty>
			<Flammability>0</Flammability>
		</statBases>
		<altitudeLayer>Floor</altitudeLayer>
		<passability>Standable</passability>
		<selectable>false</selectable>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<building>
			<isEdifice>false</isEdifice>
			<deconstructible>false</deconstructible>
		</building>
	</ThingDef>

</ThingDefinitions>